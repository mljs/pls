{
  "name": "ml-pls",
  "version": "4.3.1",
  "description": "Partial least squares library",
  "main": "./lib/index.js",
  "module": "./lib-esm/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "src",
    "lib-esm"
  ],
  "scripts": {
    "bench": "node ./benchmark/transpose.js",
    "build": "cheminfo-build --entry src/index.js --root PLS",
    "check-types": "tsc --noEmit",
    "clean": "rimraf lib lib-esm",
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "npm run tsc",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "test": "npm run test-coverage && npm run eslint && npm run prettier && npm run check-types",
    "test-coverage": "jest --coverage",
    "test-only": "jest",
    "tsc": "npm run clean && npm run tsc-cjs && npm run tsc-esm",
    "tsc-cjs": "tsc --project tsconfig.cjs.json",
    "tsc-esm": "tsc --project tsconfig.esm.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mljs/pls.git"
  },
  "keywords": [
    "partial",
    "least",
    "squares",
    "projection",
    "latent",
    "structures",
    "machine",
    "learning"
  ],
  "author": "Jefferson Hern√°ndez",
  "contributors": [
    "Julien Wist"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mljs/pls/issues"
  },
  "homepage": "https://github.com/mljs/pls",
  "dependencies": {
    "is-any-array": "^2.0.1",
    "ml-array-mean": "^1.1.6",
    "ml-confusion-matrix": "^2.0.0",
    "ml-cross-validation": "^1.3.0",
    "ml-matrix": "^6.10.4",
    "ml-roc-multiclass": "^0.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "benchmark": "^2.1.4",
    "cheminfo-build": "^1.2.0",
    "eslint": "^8.48.0",
    "eslint-config-cheminfo-typescript": "^12.0.4",
    "jest": "^29.6.4",
    "jest-matcher-deep-close-to": "^3.0.2",
    "ml-dataset-iris": "^1.2.1",
    "ml-dataset-metadata": "^0.3.0",
    "ml-kernel": "^3.0.0",
    "papaparse": "^5.4.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}